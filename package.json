{
    "name": "modern-legacy-build",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "umd": "dist/index.umd.js",
    "files": [
        "dist",
        "lib"
    ],
    "scripts": {
        "start": "npx http-server",
        "clean": "rimraf dist",
        "build:rollup": "run-s clean && run-p build:es build:cjs build:umd build:lib:es ",
        "build:es": "NODE_ENV=es rollup -c",
        "build:cjs": "NODE_ENV=cjs rollup -c",
        "build:umd": "NODE_ENV=umd rollup -c",
        "build:lib:es": "BABEL_ENV=es babel src -d lib",
        "prepare": "npm run build:rollup",
        "demo": "node demo/index.js"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.12.8",
        "@babel/core": "^7.12.9",
        "@babel/node": "^7.12.6",
        "@babel/preset-env": "^7.12.7",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "babel-jest": "^26.6.3",
        "jest": "^26.6.3",
        "nodemon": "^2.0.6",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "rollup": "^2.34.2",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-filesize": "^9.1.0"
    },
    "dependencies": {
        "axios": "^0.21.0",
        "dotenv": "^8.2.0"
    },
    "jest": {
        "collectCoverageFrom": [
            "**/*.{js,jsx}",
            "!**/node_modules/**",
            "!**/vendor/**"
        ]
    }
}
